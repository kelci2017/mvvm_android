            <EditText
                android:id="@+id/name_sixth"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_marginLeft="-40dp"
                android:layout_weight="1"
                android:background="@drawable/edittext_border_round_corner"
                android:ems="5"
                android:inputType="textPersonName"
                app:layout_constraintLeft_toRightOf="@id/textView9"
                app:layout_constraintBottom_toBottomOf="parent" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/seventh_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/sixth_layout">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="175dp"
                android:layout_weight="1"
                android:text="7."
                app:layout_constraintRight_toLeftOf="@id/guideLineIdCenter"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/name_seventh"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:layout_marginLeft="-40dp"
                android:layout_weight="1"
                android:background="@drawable/edittext_border_round_corner"
                android:ems="5"
                android:inputType="textPersonName"
                app:layout_constraintLeft_toRightOf="@id/textView10"
                app:layout_constraintBottom_toBottomOf="parent" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/eighth_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/seventh_layout">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="175dp"
                android:layout_weight="1"
                android:text="8."
                app:layout_constraintRight_toLeftOf="@id/guideLineIdCenter"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/name_eighth"
                android:layout_width="wrap_content"
